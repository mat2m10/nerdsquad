<div class="container">
  <h2>Add a token to your game</h2>
  <%= simple_form_for([@game, @token]) do |f| %>
    <%= f.input :name %>
    <%= f.input :photo, as: :file, required: true %>
    <%= f.input :number_of_tokens, collection: 1..30 %>
    <button class="big-button"><%= f.button :submit %></button>
  <% end %>
</div>
